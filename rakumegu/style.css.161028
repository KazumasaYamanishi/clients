.box-alert {
    margin-bottom: 20px;
    padding: 15px;
    background-color: #f8f4d3;
    border: 10px solid #7e5000;
    border-radius: 4px;
}

.box-alert > *:last-child {
    margin-bottom: 0;
}

.box-alert ul,
.box-alert ol {
    padding-left: 0;
    text-align: left;
}

.box-alert ul li {
    margin-bottom: 1em;
    text-indent: -1em;
    padding-left: 1em;
    font-size: 12px;
    line-height: 1.4;
}

.box-alert > p {
    margin-bottom: 20px;
}

.box-alert.alert-question {
    background-color: #f5a69b;
}

.box-alert.alert-answer {
    background-color: #e7ebf9;
}

@media (min-width: 768px) {
    .box-alert {
        padding: 60px;
    }
    .box-alert ul li {
        margin-left: 40px;
        font-size: 14px;
    }
}


