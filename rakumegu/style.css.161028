.search-title {
    color: #f4c700;
    margin: 40px 0 20px 0;
    font-weight: 600;
}

.hanbai {
    padding: 10px 20px;
    font-weight: 600;
    background-color: #df4473;
    border-radius: 4px;
    -webkit-transition: all .3s;
    transition: all .3s;
    color: #fff;
}

.kanbai {
    padding: 10px 20px;
    font-weight: 600;
    background-color: #f58e0c;
    border-radius: 4px;
    -webkit-transition: all .3s;
    transition: all .3s;
    color: #fff;
}

.breadcrumb {
    padding: 8px;
    margin: 20px 0;
    list-style: none;
    background-color: #f8f6e7;
}

.breadcrumb ul {
    font-size: 0.9em;
}

.breadcrumb ul li {
    display: inline;
    list-style: none;
    background-color: #efedcc;
    border-radius: 4px;
    margin: 0 5px;
    padding: 10px 10px 10px 15px;
}

.breadcrumb ul li:last-child {
    background-color: #efedcc;
    padding: 10px 15px;
}

.breadcrumb ul li:first-child:before {
    content: none;
}

.breadcrumb ul li:before {
    content: '›';
    margin: 0 5px 0 0;
}

.breadcrumb ul li > a {
    color: #333;
    margin-right: 5px;
    text-decoration: none;
}

.breadcrumb ul li a:hover {
    color: #efedcc;
    text-decoration: underline;
}

@media screen and (max-width: 480px) {
    .breadcrumb ul li span.e-title {
        display: block;
    }
}

.saikensaku {
    font-size: 0.7em;
    margin: 0 0 10px 20px;
    padding: 10px 20px;
    background-color: #f4c700;
    border-radius: 4px;
}

.saikensaku a {
    color: #fff;
}

.postcount {
    color: #f4c700;
    margin: 0 20px;
    font-size: 1.5em;
    font-weight: 300;
    text-align: center;
}

.postcountnbr {
    font-size: 2.0em;
    font-weight: 600;
    padding: 0 10px;
}

.btn-toagency {
    color: #fff;
    background-color: #f4c700;
    border-color: #f4c700;
    margin-top: 20px;
}

.planitem {
    font-size: 22px;
    border-bottom: 1px solid #f3c600;
    line-height: 1.3;
    padding-bottom: 10px;
    margin-top: 0px;
}

.planitem i {
    color: #f3c600;
}

.serch-agency {
    font-color: #ab1f1f;
}

.yellow-light {
    margin-bottom: 30px;
    padding: 40px 50px 40px 50px;
    border: 3px solid #f3c600;
}

.label-success {
    padding: .6em;
}

.table-sercharea {
    border-top: 1px solid #ddd;
}

.table-sercharea th {
    padding: 10px;
}

.table-sercharea td {
    padding: 10px;
}

.table-sercharea > tbody > tr > td,
.table-sercharea > tbody > tr > th,
.table-sercharea > thead > tr > td,
.table-sercharea > thead > tr > th {
    padding: 10px 20px 10px 20px;
    vertical-align: middle;
    border-bottom: 1px solid #ddd;
}

.tourbun {
